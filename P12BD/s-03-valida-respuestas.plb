set serveroutput on
set linesize 200
col host format A20
col os_user format A20
col db_user format A20
select to_char(sysdate,'dd/mm/yyyy HH24:mi:ss') session_time, 
SYS_CONTEXT('USERENV','HOST') host,SYS_CONTEXT('USERENV','OS_USER') os_user,
username db_user
from user_users;
